<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.github.emilg1101.marketplace.fxclient.controller.MainController">
    <Button fx:id="load" onMouseClicked="#onClickLoad" text="Load" AnchorPane.leftAnchor="10"
            AnchorPane.topAnchor="10"/>
    <fx:include source="productTable.fxml" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10"
                AnchorPane.rightAnchor="10" AnchorPane.topAnchor="40"/>
    <Button fx:id="add" onMouseClicked="#onClickAdd" layoutX="63.0" layoutY="10.0" mnemonicParsing="false"
            text="Add Product"/>
</AnchorPane>
