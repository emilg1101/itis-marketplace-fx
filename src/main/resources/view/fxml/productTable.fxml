<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="productTable" prefWidth="350.0"
           xmlns="http://javafx.com/javafx/8.0.121"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.github.emilg1101.marketplace.fxclient.controller.ProductTableController">
    <columns>
        <TableColumn fx:id="id" prefWidth="30.0" text="ID"/>
        <TableColumn fx:id="title" prefWidth="250.0" text="Title"/>
        <TableColumn fx:id="price" prefWidth="70.0" text="Price"/>
    </columns>
</TableView>